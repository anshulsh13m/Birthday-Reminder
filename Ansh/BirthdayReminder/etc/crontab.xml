        <job instance="Ansh\BirthdayReminder\Cron\BirthdayReminder" method="execute" name="ansh_birthdayreminder_cron">
            <!--Cron set at every 6AM -->
            <schedule>* 6 * * *</schedule>
        </job>
    </group>
</config>